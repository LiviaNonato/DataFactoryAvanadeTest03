{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfc-aula-gorvernaca-03"
		},
		"AzureDataLakeStorageAula3_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageAula3'"
		},
		"AzureMySqlSakila3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySqlSakila3'"
		},
		"MySqlCarloca3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlCarloca3'"
		},
		"AzureDataLakeStorageAula3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testefiap2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfc-aula-gorvernaca-03_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageAula3_accountKey": {
						"value": "[parameters('AzureDataLakeStorageAula3_accountKey')]"
					},
					"AzureMySqlSakila3_connectionString": {
						"value": "[parameters('AzureMySqlSakila3_connectionString')]"
					},
					"MySqlCarloca3_connectionString": {
						"value": "[parameters('MySqlCarloca3_connectionString')]"
					},
					"AzureDataLakeStorageAula3_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageAula3_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}